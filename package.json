{
  "name": "hull-datanyze",
  "description": "Enrich Customer data with Datanyze",
  "version": "0.2.2",
  "homepage": "https://github.com/hull-ships/hull-datanyze",
  "license": "MIT",
  "main": "bin/start",
  "author": {
    "name": "Hull",
    "email": "contact@hull.io",
    "url": "https://github.com/hull"
  },
  "engines": {
    "node": "6.x",
    "npm": "3.x"
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/hull-ships/hull-datanyze"
  },
  "bugs": {
    "url": "https://github.com/hull-ships/hull-datanyze/issues"
  },
  "scripts": {
    "build": "npm run clean && npm run build:server",
    "build:server": "babel server -d lib",
    "start": "node ./lib",
    "start:dev": "COMBINED=true babel-watch -L server",
    "test": "npm run test:lint && npm run test:flow && npm run test:units && npm run test:specs",
    "test:units": "NODE_ENV=test mocha --require babel-register -R spec ./tests/* ./tests/**/*",
    "test:specs": "mocha --require babel-register -R spec ./specs",
    "test:lint": "eslint server",
    "test:flow": "flow check",
    "clean": "./node_modules/.bin/rimraf dist; ./node_modules/.bin/rimraf lib",
    "update": "./node_modules/.bin/updtr",
    "postinstall": "npm run build"
  },
  "keywords": [
    "hull",
    "ship",
    "datanyze",
    "hull-ship"
  ],
  "dependencies": {
    "babel-cli": "^6.10.1",
    "babel-preset-es2015-node6": "^0.4.0",
    "babel-preset-flow": "^6.23.0",
    "babel-preset-stage-0": "^6.5.0",
    "bluebird": "^3.4.7",
    "cache-manager-redis": "^0.4.0",
    "cheerio": "^0.22.0",
    "ejs": "^2.5.6",
    "express": "^4.14.0",
    "hull": "0.11.8",
    "kue": "^0.11.5",
    "lodash": "^4.14.1",
    "newrelic": "^1.39.1",
    "nock": "^9.0.13",
    "parse-domain": "^1.1.0",
    "request": "^2.79.0",
    "request-promise": "^4.1.1",
    "restler": "^3.4.0",
    "rimraf": "^2.5.4",
    "winston-logstash": "^0.3.0"
  },
  "devDependencies": {
    "babel-core": "^6.11.4",
    "babel-eslint": "^6.1.2",
    "babel-register": "^6.11.6",
    "babel-watch": "^2.0.5",
    "chai": "^3.5.0",
    "eslint": "^2.13.1",
    "eslint-config-airbnb-base": "^3.0.1",
    "eslint-plugin-import": "^1.9.2",
    "flow-bin": "^0.45.0",
    "minihull": "^0.0.4",
    "mocha": "^3.2.0",
    "nodemon": "^1.9.2",
    "sinon": "^2.3.8",
    "updtr": "^0.1.15",
    "webpack": "^1.13.1"
  }
}
