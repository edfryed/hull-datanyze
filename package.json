{
  "name": "hull-datanyze",
  "description": "Enrich Customer data with Datanyze",
  "version": "0.3.1",
  "homepage": "https://github.com/hull-ships/hull-datanyze",
  "license": "MIT",
  "main": "bin/start",
  "author": {
    "name": "Hull",
    "email": "contact@hull.io",
    "url": "https://github.com/hull"
  },
  "engines": {
    "node": "6.12.x",
    "npm": "3.10.x",
    "yarn": "1.3.x"
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/hull-ships/hull-datanyze"
  },
  "bugs": {
    "url": "https://github.com/hull-ships/hull-datanyze/issues"
  },
  "scripts": {
    "ngrok": "ngrok http 8082 --region eu --subdomain datanyze",
    "build": "npm run clean && npm run build:server",
    "build:server": "babel server -d lib",
    "start": "node ./lib",
    "start:dev": "COMBINED=true babel-watch -L server",
    "test": "npm run test:lint && npm run test:unit && npm run test:integration",
    "test:unit": "NODE_ENV=test mocha --require babel-register -R spec ./test/unit/*.js",
    "test:integration": "mocha --require babel-register -R spec ./test/integration/*.js",
    "test:lint": "eslint server",
    "test:flow": "flow check",
    "test:coverage": "nyc --babel-cache --hook-run-in-context false npm test",
    "clean": "rimraf dist; rimraf lib",
    "update": "updtr",
    "postinstall": "npm run build"
  },
  "keywords": [
    "hull",
    "ship",
    "datanyze",
    "hull-ship"
  ],
  "dependencies": {
    "babel-cli": "^6.10.1",
    "babel-preset-es2015-node6": "^0.4.0",
    "babel-preset-flow": "^6.23.0",
    "babel-preset-stage-0": "^6.5.0",
    "bluebird": "^3.4.7",
    "cache-manager-redis": "^0.4.0",
    "cheerio": "^0.22.0",
    "ejs": "^2.5.6",
    "express": "^4.14.0",
    "hull": "0.13.10",
    "kue": "^0.11.5",
    "lodash": "^4.14.1",
    "newrelic": "^2.7.1",
    "nock": "^9.0.13",
    "parse-domain": "^1.1.0",
    "request": "^2.79.0",
    "request-promise": "^4.1.1",
    "restler": "^3.4.0",
    "rimraf": "^2.5.4",
    "winston-logstash": "^0.3.0"
  },
  "devDependencies": {
    "babel-core": "^6.11.4",
    "babel-eslint": "^8.2.1",
    "babel-register": "^6.11.6",
    "babel-watch": "^2.0.5",
    "chai": "^3.5.0",
    "eslint": "^4.17.0",
    "eslint-config-airbnb-base": "^12.1.0",
    "eslint-plugin-flowtype": "^2.44.0",
    "eslint-plugin-flowtype-errors": "^3.3.7",
    "eslint-plugin-import": "^2.8.0",
    "eslint-plugin-jest": "^21.12.1",
    "flow-bin": "^0.65.0",
    "minihull": "^2.0.0",
    "mocha": "^3.2.0",
    "nyc": "^11.0.3",
    "sinon": "^2.3.8",
    "updtr": "^0.1.15",
    "webpack": "^1.13.1"
  }
}
